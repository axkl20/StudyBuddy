<!doctype html>
<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Gruppo" />
<%- include("header", {title: title}); %>

<h2>Subjects and available decks</h2>

<table>
    <tr>
        <th>Subjects</th>
    </tr>
<% for (const row of res) { %>
    <tr>
        <td><a href="/studybuddy/subjects/<%= row.subjectname %>"><%= row.subjectname %> (<%= row.available %>)</a></td>
    </tr>
<% }; %>
</table>

<%- include("footer"); %>
